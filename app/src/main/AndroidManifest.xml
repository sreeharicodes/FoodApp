<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wordpress.sreeharilive.foodapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.CartActivity" />
        <activity android:name=".activity.ItemsListActivity" />
        <activity android:name=".activity.FoodDescriptionActivity" />
        <activity android:name=".activity.CheckoutActivity" />
        <activity android:name=".activity.SplashActivity" />
        <activity android:name=".activity.HistoryActivity"></activity>
    </application>

</manifest>